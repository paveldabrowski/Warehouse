	<queryString language="SQL">
		<![CDATA[SELECT t.roadCard_number,
t.user,
r.date,
r.clients,
r.loader,
r.driver,
r.registration_number,
r.weight,
r.gross_value,
me.my_name,
me.my_street,
me.my_zip_code,
me.my_city,
me.my_country,
me.my_NIP,
me.my_phone_number,
me.my_website,
me.my_bank_account,
u.name,
u.lastName,
CONCAT(l.name, ' ', l.lastName) AS loaderName
FROM  warehouse.`roadCardToReport`  t
JOIN  warehouse.`roadCards`  r 
ON t.roadCard_number = r.card_id
JOIN  warehouse.`myCompany` me
ON me.`my_id` = 1
JOIN  warehouse.users u
ON r.driver = u.YWN
JOIN warehouse.users l
ON r.loader = l.YWN
 WHERE `no.` = 1]]>
	</queryString>
