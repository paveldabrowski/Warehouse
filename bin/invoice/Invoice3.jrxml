	<queryString language="SQL">
		<![CDATA[SELECT
	d.order_id,
	d.client_id,
	d.amount_of_products,
	d.value,
	d.gross_value,
	d.vat_value,
	d.invoice_number,	
	me.my_name,
	me.my_street,
	me.my_zip_code,
	me.my_city,
	me.my_country,
	me.my_NIP,
	me.my_phone_number,
	me.my_website,
	me.my_bank_account,
	c.name,
	c.street,
	c.zip_code,
	c.city,
	c.country,
	c.NIP,
	c.phone_number
FROM warehouse.`dataToInvoice` d
JOIN   warehouse.`myCompany` me
ON   me.my_id = '1' 
JOIN  warehouse.clients c
ON d.client_id = c.id]]>
	</queryString>
